<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\LwdHasDisability" table="lwd_has_disability">
    <indexes>
      <index name="fk_lwd_has_disability_disability1_idx" columns="iddisability"/>
      <index name="fk_lwd_has_disability_lwd1_idx" columns="idlwd"/>
      <index name="fk_lwd_has_disability_disability_has_level1_idx" columns="idlevel,iddisability"/>
    </indexes>
    <id name="iddisability" association-key="true"/>
    <id name="idlwd" association-key="true"/>
    <field name="assessedBy" type="simplearray" column="assessed_by" nullable="false"/>
    <field name="dateAssessed" type="date" column="date_assessed" nullable="false"/>
    <field name="identifiedBy" type="simplearray" column="identified_by" nullable="false"/>
    <field name="identificationDate" type="date" column="identification_date" nullable="false"/>
    <field name="caseDescription" type="string" column="case_description" length="150" nullable="true"/>
    <one-to-one field="iddisability" target-entity="Disability">
      <join-columns>
        <join-column name="iddisability" referenced-column-name="iddisability"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idlwd" target-entity="Lwd">
      <join-columns>
        <join-column name="idlwd" referenced-column-name="idlwd"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idlevel" target-entity="DisabilityHasLevel">
      <join-columns>
        <join-column name="idlevel" referenced-column-name="idlevel"/>
        <join-column name="iddisability" referenced-column-name="iddisability"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
