<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\LwdBelongsToSchool" table="lwd_belongs_to_school">
    <indexes>
      <index name="fk_lwd_has_school_lwd1_idx" columns="idlwd"/>
      <index name="fk_lwd_has_school_school1_idx" columns="emiscode"/>
    </indexes>
    <id name="year" type="date" column="year"/>
    <id name="idlwd" association-key="true"/>
    <id name="emiscode" association-key="true"/>
    <one-to-one field="emiscode" target-entity="School">
      <join-columns>
        <join-column name="emiscode" referenced-column-name="emiscode"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idlwd" target-entity="Lwd">
      <join-columns>
        <join-column name="idlwd" referenced-column-name="idlwd"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
