<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SchoolExit" table="school_exit">
    <indexes>
      <index name="fk_dropout_school1_idx" columns="school_emiscode"/>
      <index name="fk_dropout_lwd1_idx" columns="lwd_idlwd"/>
    </indexes>
    <id name="year" type="date" column="year"/>
    <id name="lwdlwd" association-key="true"/>
    <id name="schoolEmiscode" association-key="true"/>
    <field name="reason" type="simplearray" column="reason" nullable="false"/>
    <field name="otherReason" type="string" column="other_reason" length="20" nullable="true"/>
    <one-to-one field="schoolEmiscode" target-entity="School">
      <join-columns>
        <join-column name="school_emiscode" referenced-column-name="emiscode"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="lwdlwd" target-entity="Lwd">
      <join-columns>
        <join-column name="lwd_idlwd" referenced-column-name="idlwd"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
