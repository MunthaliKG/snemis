<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SchoolHasSnt" table="school_has_snt">
    <indexes>
      <index name="fk_school_has_snt_snt1_idx" columns="idsnt"/>
      <index name="fk_school_has_snt_school1_idx" columns="emiscode"/>
    </indexes>
    <id name="year" type="date" column="year"/>
    <id name="emiscode" association-key="true"/>
    <id name="idsnt" association-key="true"/>
    <one-to-one field="idsnt" target-entity="Snt">
      <join-columns>
        <join-column name="idsnt" referenced-column-name="idsnt"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="emiscode" target-entity="School">
      <join-columns>
        <join-column name="emiscode" referenced-column-name="emiscode"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
