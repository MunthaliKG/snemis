<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Snt" table="snt">
    <id name="idsnt" type="integer" column="idsnt">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sfirstName" type="string" column="sfirst_name" length="25" nullable="false"/>
    <field name="slastName" type="string" column="slast_name" length="25" nullable="false"/>
    <field name="sinitials" type="string" column="sinitials" length="25" nullable="false"/>
    <field name="sSex" type="string" column="s_sex" nullable="false"/>
    <field name="qualification" type="string" column="qualification" nullable="false"/>
    <field name="speciality" type="string" column="speciality" nullable="false"/>
    <field name="yearStarted" type="date" column="year_started" nullable="false"/>
  </entity>
</doctrine-mapping>
