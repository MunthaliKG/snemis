<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Zone" table="zone">
    <indexes>
      <index name="fk_zone_district1_idx" columns="district_iddistrict"/>
    </indexes>
    <id name="idzone" type="integer" column="idzone">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="zoneName" type="string" column="zone_name" length="20" nullable="true"/>
    <many-to-one field="districtdistrict" target-entity="District">
      <join-columns>
        <join-column name="district_iddistrict" referenced-column-name="iddistrict"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
