<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Lwd" table="lwd">
    <indexes>
      <index name="fk_lwd_guardian1_idx" columns="idguardian"/>
    </indexes>
    <id name="idlwd" type="integer" column="idlwd">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="distanceToSchool" type="integer" column="distance_to_school" nullable="true"/>
    <field name="firstName" type="string" column="first_name" length="25" nullable="false"/>
    <field name="lastName" type="string" column="last_name" length="25" nullable="false"/>
    <field name="initials" type="string" column="initials" length="8" nullable="true"/>
    <field name="homeAddress" type="string" column="home_address" length="100" nullable="false"/>
    <field name="sex" type="string" column="sex" nullable="false"/>
    <field name="dob" type="date" column="dob" nullable="false"/>
    <field name="guardianRelationship" type="string" column="guardian_relationship" nullable="false"/>
    <many-to-one field="idguardian" target-entity="Guardian">
      <join-columns>
        <join-column name="idguardian" referenced-column-name="idguardian"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
